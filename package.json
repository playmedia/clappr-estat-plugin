{
  "name": "clappr-estat-plugin",
  "version": "0.2.2",
  "description": "Mediametrie eStat streaming plugin for Clappr player.",
  "main": "dist/clappr-estat-plugin.js",
  "scripts": {
    "dist": "npm run dist-main && npm run dist-min",
    "dist-main": "webpack --progress",
    "dist-min": "webpack --progress -p --optimize-mangle --optimize-dedupe --output-filename clappr-estat-plugin.min.js",
    "watch": "webpack -w",
    "demo": "http-cli -r ./ -a 0.0.0.0 -p 8090",
    "check": "david"
  },
  "author": "Karim Slimani <karim@playmedia.fr>",
  "license": "MIT",
  "repository": "playmedia/clappr-estat-plugin",
  "devDependencies": {
    "babel-core": "6.14.0",
    "babel-loader": "6.2.5",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.14.0",
    "clean-webpack-plugin": "0.1.10",
    "david": "9.0.0",
    "http-cli": "1.1.0",
    "webpack": "1.13.2"
  }
}
